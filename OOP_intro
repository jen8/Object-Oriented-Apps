class Student:
    
    def __init__(self, name, grade, age):
        self.name = name
        self.grade = grade
        self.age = age

chris = Student('Chris', 85, 30)
rebecca = Student('Rebecca', 80, 15)

print(chris.name)
print(rebecca.name)

print(chris.grade)
print(rebecca.grade)

print(chris.age)
print(rebecca.age)
